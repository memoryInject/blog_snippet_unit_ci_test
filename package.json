{
  "name": "blog_snippet_unit_ci_test",
  "version": "1.0.0",
  "description": "simple blog for unit testing and CI ",
  "main": "server.js",
  "scripts": {
    "start": "node server/server",
    "test-server": "jest --no-cache",
    "test-client": "npm test --prefix client -- --watchAll=false",
    "test": "concurrently \"npm run test-server\" \"npm run test-client\"",
    "server": "nodemon server/server",
    "server-test": "cross-env NODE_ENV=test nodemon server/server",
    "client": "npm start --prefix client",
    "client-test": "cross-env BROWSER=none PORT=3006 npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev-test": "concurrently \"npm run server-test\" \"npm run client-test\"",
    "migrate": "node-pg-migrate",
    "migrate-up": "npm run migrate up",
    "migrate-up-one": "npm run migrate-up 1",
    "migrate-down": "npm run migrate down",
    "migrate-down-all": "npm run migrate-down 0",
    "migrate-up-test": "cross-env DATABASE_URL=postgres://postgres:1234@localhost:5432/blogs_test npm run migrate up",
    "migrate-down-test": "cross-env DATABASE_URL=postgres://postgres:1234@localhost:5432/blogs_test npm run migrate-down 0",
    "create-table-users": "npm run migrate create table users",
    "create-table-blogs": "npm run migrate create table blogs",
    "insert-user-data": "node ./data/insert-users.js",
    "destroy-user-data": "node ./data/insert-users.js -d",
    "insert-blog-data": "node ./data/insert-blogs.js",
    "destroy-blog-data": "node ./data/insert-blogs.js -d",
    "data-import": "npm run insert-user-data && npm run insert-blog-data",
    "data-destroy": "npm run destroy-user-data && npm run destroy-blog-data",
    "data-import-test": "cross-env NODE_ENV=test npm run insert-user-data && cross-env NODE_ENV=test npm run insert-blog-data",
    "data-destroy-test": "cross-env NODE_ENV=test npm run destroy-user-data && cross-env NODE_ENV=test npm run destroy-blog-data",
    "cypress-open": ".\\node_modules\\.bin\\cypress open"
  },
  "author": "Mahesh",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dedent": "^0.7.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jest": "^26.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^5.9.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.5.0",
    "cypress-dark": "^1.7.14",
    "nodemon": "^2.0.7"
  }
}
